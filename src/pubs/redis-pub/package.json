    "copyfiles": "^1.2.0",
    "diagnostic-source": "../../diagnostic-source",
    "mocha": "^3.2.0",
    "redis": "^2.7.1",
    "rimraf": "^2.6.1",
    "tslint": "^5.0.0",
    "typescript": "^2.2.1",
    "zone.js": "^0.8.5"
  },
  "peerDependencies": {
    "diagnostic-source": "*"
  },
  "files": [
    ".dist/**/*",
    "LICENSE",
    "README.md",
    "package.json"
